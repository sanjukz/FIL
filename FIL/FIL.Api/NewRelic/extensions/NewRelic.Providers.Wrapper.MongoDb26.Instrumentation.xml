				<exactMethodMatcher methodName="CreateCollectionAsync" />
				<exactMethodMatcher methodName="CreateViewAsync" />
				<exactMethodMatcher methodName="DropCollectionAsync" />
				<exactMethodMatcher methodName="ListCollectionsAsync" />
